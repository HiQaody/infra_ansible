expose:
  type: nodePort
  tls:
    enabled: false
  nodePort:
    name: harbor
    ports:
      http:
        port: {{ harbor_nodeport }}
      https:
        port: 32443
harborAdminPassword: "{{ harbor_admin_password }}"
persistence:
  enabled: true
  persistentVolumeClaim:
    registry: { size: 5Gi }
    chartmuseum: { size: 5Gi }
    jobservice: { size: 1Gi }
    database: { size: 1Gi }
    redis: { size: 1Gi }
    trivy: { size: 5Gi }